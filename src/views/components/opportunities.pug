.opportunities
    h1.opportunities__titlehead.title!=app.opportunities.title
    .opportunities__wr.containner-pad
        //- img(src="/static/img/icons/logoDecore.png", alt="").opportunities__logo
        img(src="/static/img/icons/logoC.svg", alt="").opportunities__logo
        .opportunities__inner
            .opportunities__left
                ul.opportunities__list 
                    li.opportunities__item!=app.opportunities.li_1
                    li.opportunities__item!=app.opportunities.li_2
                    li.opportunities__item!=app.opportunities.li_3
                    li.opportunities__item!=app.opportunities.li_4
                    li.opportunities__item!=app.opportunities.li_5
                    li.opportunities__item!=app.opportunities.li_6
            .opportunities__right
                form(action="#").opportunities__form
                    h3.opportunities__title.title!=app.opportunities.title_2
                    .opportunities__input-wr
                        input(type="text" placeholder="Type your nickname here...").opportunities__input.input
                        span.opportunities__input-tooltip not found
                    input(type="submit", value="Get Started").opportunities__btn.btn-base-shadow